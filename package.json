{
  "name": "backend-test",
  "version": "1.0.0",
  "repository": "git@github.com:shopbonsai/backend-test.git",
  "author": "Mackie Drew <mackie@shopbonsai.ca>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "pretest": "npm run posttest && MONGODB_VERSION=4.x MONGODB_PORT=27019 MONGODB_TOPOLOGY=standalone mongodb-runner start",
    "test": "jest .",
    "posttest": "MONGODB_VERSION=4.x MONGODB_PORT=27019 MONGODB_TOPOLOGY=standalone mongodb-runner stop",
    "lint": "tslint --project '.'",
    "prettify": "prettier --config ./.prettierrc.json --write './source/**/*.ts'",
    "sane": "yarn lint && yarn prettify && yarn test",
    "start": "ts-node source/main.ts",
    "watch": "nodemon --watch 'source/**/*.ts' --ignore 'source/**/*.test.ts' --exec 'ts-node' source/main.ts dist/main.js",
    "build": "tsc"
  },
  "dependencies": {
    "apollo-server": "2.5.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "graphql": "14.3.0",
    "graphql-fields": "^2.0.3",
    "mongodb": "3.2.4",
    "mongoose": "5.5.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.5.2",
    "type-graphql": "^0.17.5",
    "typegoose": "5.6.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/graphql": "14.2.0",
    "@types/graphql-fields": "^1.3.2",
    "@types/jest": "24.0.18",
    "@types/mongodb": "3.1.26",
    "@types/mongoose": "5.5.0",
    "@types/node": "12.0.0",
    "jest": "24.8.0",
    "mongodb-runner": "^4.7.2",
    "nodemon": "1.19.0",
    "prettier": "1.17.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  }
}
